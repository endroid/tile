{
    "name": "endroid/tile",
    "description": "Endroid Tile",
    "keywords": ["endroid", "delft", "blue", "tile", "php"],
    "homepage": "https://github.com/endroid/tile",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "imagine/imagine": "^1.2.4",
        "symfony/routing": "^5.4||^6.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Tile\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
